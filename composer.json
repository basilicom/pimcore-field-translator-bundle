{
  "name": "basilicom/pimcore-field-translator-bundle",
  "license": "GPL-3.0-or-later",
  "type": "pimcore-bundle",
  "description": "A Pimcore bundle to provide translation helpers for fields in the Pimcore Backend.",
  "keywords": [
    "pimcore",
    "translation",
    "translator",
    "localized fields",
    "localizedFields"
  ],
  "homepage": "https://basilicom.de",
  "authors": [
    {
      "name": "Alexander Heidrich",
      "email": "alexander.heidrich@basilicom.de"
    },
    {
      "name": "Basilicom GmbH",
      "homepage": "https://basilicom.de"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.0",
    "google/cloud-translate": "^1.12",
    "pimcore/pimcore": ">=10.0"
  },
  "suggest": {},
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Basilicom\\FieldTranslatorBundle\\": "./src/"
    },
    "exclude-from-classmap": [
      "tests"
    ]
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Basilicom\\FieldTranslatorBundle\\BasilicomFieldTranslatorBundle"
      ]
    }
  }
}
